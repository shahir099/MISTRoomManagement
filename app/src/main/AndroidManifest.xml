<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kanak.mistroommanagement">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".login" />
        <activity android:name=".Registration" />
        <activity android:name=".tower" />
        <activity android:name=".floor" />
        <activity android:name=".rooms">

        </activity>
        <activity android:name=".room_info" />
        <activity android:name=".room_schedule" />
        <activity android:name=".SetRoutine"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".InsertData">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>
    </application>

</manifest>